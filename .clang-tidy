Checks: "-*,performance-*,modernize-*,readability-*,bugprone-*,-modernize-use-trailing-return-type"
WarningsAsErrors: false
HeaderFilterRegex: ".*"
FormatStyle: "file"
CheckOptions:
  - key: readability-identifier-naming.ClassCase
    value: CamelCase
  - key: readability-identifier-naming.FunctionCase
    value: camelBack
